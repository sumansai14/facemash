$.ajax({
  type:'GET',
  contentType: 'image/png',
  success: function() {    
    $('#test img').attr('src','<%= j @profile.picture.url(:small)  %>');
    $('#thumb img').attr('src','<%= j @profile.picture.url(:thumbnail)  %>');
    $('form').get(0).reset();    
  },
  error: function(){
  	$('img').attr('src','<%= j @profile.picture.url(:small)  %>');
  }
});


$('#profile_name').remove();
$('#name').show();

